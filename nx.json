{
  "npmScope": "workspace",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "eslintrc.js": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "example": {
      "tags": ["scope:app-example-web"]
    },
    "example-e2e": {
      "tags": ["scope:e2e"],
      "implicitDependencies": ["example"]
    },
    "playground-shared": {
      "tags": ["scope:web"],
      "implicitDependencies": ["ngx-highlight", "ngx-search-bar"]
    },
    "ngx-highlight": {
      "tags": ["scope:shared", "scope:published", "type:ui"]
    },
    "ngx-search-bar": {
      "tags": ["scope:published", "scope:shared", "type:ui"],
      "implicitDependencies": ["ngx-highlight"]
    },
    "ui-base": {
      "tags": ["scope:shared", "type:ui"]
    },
    "util-angular": {
      "tags": ["scope:shared", "type:util"]
    },
    "data-router-store": {
      "tags": ["scope:shared", "type:data"]
    }
  }
}
