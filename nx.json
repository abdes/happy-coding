{
  "npmScope": "happy-coding",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "eslintrc.js": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "example": {
      "tags": ["app", "client", "web"]
    },
    "example-e2e": {
      "tags": ["e2e"],
      "implicitDependencies": ["example"]
    },
    "ngx-highlight": {
      "tags": ["lib", "shared", "publishable"]
    },
    "ngx-search-bar": {
      "tags": ["lib", "publishable", "shared"]
    },
    "playground-shared": {
      "tags": ["app", "web", "client"]
    }
  }
}
